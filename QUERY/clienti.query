SELECT T2.CKY_CNT CONTO,T2.CKY_CNT_AGENTE CODICE_AGE1,T4.CDS_CNT_RAGSOC AGE1,T3.V1C1_CODICE_AGENTE_2 CODICE_AGE2,T5.CDS_CNT_RAGSOC AGE2,T2.NGB_ZONA ZO,T6.CDS_ZONA_CLFR ZO_DESC,T2.NGB_LIS LIS FROM G_RUDT T1
  LEFT JOIN PICO T2 ON (T2.AZIENDA='PAT' AND T2.CKY_CNT=T1.CKY_CNT)
  LEFT JOIN PICO_VID T3 ON (T3.AZIENDA='PAT' AND T3.CKY_CNT=T1.CKY_CNT)
  LEFT JOIN G_RUDT T4 ON (T4.AZIENDA='004' AND T4.CKY_CNT=T2.CKY_CNT_AGENTE)
  LEFT JOIN G_RUDT T5 ON (T5.AZIENDA='004' AND T5.CKY_CNT=T3.V1C1_CODICE_AGENTE_2)
  LEFT JOIN TABE_ZONA_CF T6 ON (T2.NGB_ZONA=T6.NKY_ZONA_CLFR AND T6.AZIENDA='PAT')
  WHERE T1.AZIENDA='004' AND T1.IFL_CNT_ANN=0 AND T1.CKY_CNT LIKE '501.%' AND (T2.CKY_CNT_AGENTE LIKE '631.%' OR T3.V1C1_CODICE_AGENTE_2 LIKE '631.%') ORDER BY CONTO